    "cookies-js": "^1.2.1",
    "lodash": "3.x",
    "moment": "2.x",
    "qs": "2.x",
    "react": "0.13.x",
    "react-bootstrap": "^0.20.1",
    "react-router": "0.13.x",
    "superagent": "0.x",
    "validator": "3.x"
  }
